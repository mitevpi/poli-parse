{
  "name": "poli-parse",
  "version": "1.1.3",
  "description": "Political news scraping & NLP parsing from web pages.",
  "main": "dist/index.js",
  "author": {
    "name": "Petr Mitev",
    "email": "p.mitevpi@gmail.com",
    "url": "https://mitevpi.com/"
  },
  "keywords": [
    "political",
    "sentiment",
    "news",
    "bias",
    "data",
    "headline"
  ],
  "files": [
    "dist",
    "src",
    "README.md",
    "samples"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mitevpi/poli-parse.git"
  },
  "license": "MIT",
  "contributors": [
    "Julia Mitev"
  ],
  "bugs": {
    "url": "https://github.com/mitevpi/poli-parse/issues"
  },
  "homepage": "https://github.com/mitevpi/poli-parse#readme",
  "jest": {
    "verbose": true
  },
  "scripts": {
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint . --fix",
    "flow": "flow check",
    "docs": "documentation build src/index.js -f md -o docs/README.md && documentation build src/** -f html -o docs",
    "postdocs": "",
    "clean": "rimraf dist",
    "flowbuild": "flow-copy-source src dist",
    "prebuild": "npm run clean && npm run flowbuild",
    "build": "babel src -d dist && npm run docs",
    "preversion": "npm run lint && npm run build",
    "version": "standard-changelog && git add CHANGELOG.md",
    "postpublish": "git push origin master --follow-tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "0.21.1",
    "cheerio": "1.0.0-rc.3",
    "he": "1.2.0",
    "sentiment": "5.0.2",
    "wordpos": "1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.7.2",
    "@babel/plugin-proposal-class-properties": "7.7.0",
    "@babel/preset-env": "7.7.1",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "documentation": "12.1.3",
    "eslint": "6.6.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-flowtype": "4.3.0",
    "eslint-plugin-flowtype-errors": "^4.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "flow-bin": "0.111.3",
    "flow-copy-source": "2.0.8",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "lint-staged": "9.4.2",
    "opn-cli": "^5.0.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.0",
    "standard-changelog": "2.0.17"
  }
}
